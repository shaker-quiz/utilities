{
  "typescript": {
    "quoteStyle": "preferSingle",
    "semiColons": "asi",
    "indentWidth": 2,
    "useTabs": false,
    "useBraces": "maintain",
    "arrowFunction.useParentheses": "preferNone",
    "lineWidth": 120,
    "importDeclaration.forceSingleLine": true
  },
  "json": {},
  "markdown": {},
  "dockerfile": {},
  "yaml": {},
  "excludes": [
    "**/node_modules",
    "**/*-lock.json"
  ],
  "plugins": [
    "https://plugins.dprint.dev/typescript-0.93.0.wasm",
    "https://plugins.dprint.dev/json-0.19.3.wasm"
  ]
}